{
  "version": 2,
  "builds": [
    {
      "src": "agent/api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "agent/api.py"
    }
  ],
  "env": {
    "GROQ_API_KEY": "@groq_api_key",
    "COHERE_API_KEY": "@cohere_api_key",
    "QDRANT_URL": "@qdrant_url",
    "QDRANT_API_KEY": "@qdrant_api_key"
  }
}
